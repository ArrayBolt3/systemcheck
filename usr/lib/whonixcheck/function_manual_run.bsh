function_manual_run() {
   if [ "$FUNCTION" = "none" ]; then
      echo "$SCRIPTNAME no function given. List of functions:" >&2
      typeset -f | awk '/ \(\) $/ && !/^main / {print $1}'
      EXIT_CODE="1"
      cleanup
      return 0
   fi
   if [ "$FUNCTION" = "" ]; then
      return 0
   fi
   $FUNCTION
   cleanup
   return 0
 }
